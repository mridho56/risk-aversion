q <- function(theta,rc,rb,rs,inst,flag) {
beta <- theta[1]
gam  <- theta[2]
m1   <- (beta*(1 + rc)^(-gam)*(1 + rb) - 1) * inst
m2   <- (beta*(1 + rc)^(-gam)*(1 + rs) - 1) * inst
m    <- cbind(m1, m2)
g    <- colMeans(m)
if (flag)
w <- t(m) %*% m/nrow(m)
else
w <- eye(ncol(m))
ret  <- 0.5*t(g) %*% inv(w) %*% g
return(ret)
}
View(q)
